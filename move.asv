function [person,map]=move(person,map)

dt = 1; %timestep 
max_step = 5;


for i=1:person.length
    if person.force_x(i)*dt > max_step
        person.x(i) = person.x(i) + max_step;
    elseif person.force_x(i)*dt < -max_step
        person.x(i) = person.x(i) - max_step;
    else
        person.x(i) = person.x(i) + person.force_x(i)*dt;
    end
end

end
